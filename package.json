{
  "name": "obsidian-publish-action",
  "version": "0.0.1",
  "private": true,
  "description": "Publish notes from Obsidian to a GitHub Pages",
  "exports": "./lib/main.js",
  "type": "module",
  "engines": {
    "node": ">=14.16"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build lib/main.js --source-map --license licenses.txt",
    "test": "mocha",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/glob": "^0.3.0",
    "@actions/io": "^1.1.2",
    "@types/mdast": "^3.0.10",
    "fp-ts": "^2.13.1",
    "front-matter": "^4.0.2",
    "glob": "^8.1.0",
    "mdast-util-wiki-link": "^0.0.2",
    "micromark-extension-wiki-link": "^0.0.4",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-stringify": "^10.0.2",
    "unist-builder": "^3.0.1",
    "validate.js": "^0.13.1",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "4.1.1",
    "@types/chai": "^4.3.4",
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "@types/sinon": "^10.0.13",
    "@typescript-eslint/parser": "^5.50.0",
    "@vercel/ncc": "^0.36.1",
    "chai": "^4.3.7",
    "eslint": "^8.39.0",
    "eslint-plugin-github": "^4.3.2",
    "fast-check": "^3.6.2",
    "memfs": "^3.4.13",
    "mocha": "^10.2.0",
    "prettier": "^2.5.1",
    "sinon": "^15.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
